{
  "name": "playmayte_backend",
  "version": "0.1.0",
  "description": "Backend for playmayte",
  "scripts": {
    "client-install": "cd client && npm install",
    "copy-env": "copyfiles -u 2 src/env/.env.test src/env/.env.development src/env/.env.production dist/env/",
    "copy-res": "copyfiles -u 1 src/resources/* dist/",
    "serve": "nodemon src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc -p .",
    "client": "cd client && npm run start",
    "dev": "concurrently \"npm run serve\" \"npm run client\"",
    "build:docker": "npm run build && docker image build -t playmayte_web .",
    "start:docker": "npm run build:docker && docker container run -d -p 8000:8000 --name playmayte_web playmayte_web",
    "docker-compose": "npm run build && docker-compose up"
  },
  "author": "Jareth Rader",
  "license": "MIT",
  "dependencies": {
    "argon2": "^0.26.2",
    "aws-sdk": "^2.699.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^4.1.2",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "express-session": "^1.17.1",
    "express-validator": "^6.4.0",
    "formidable": "^1.2.2",
    "googleapis": "^52.1.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.26.0",
    "mongoose": "^5.7.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.8",
    "nodemailer-ses-transport": "^1.5.1",
    "reallydangerous": "^2.1.0",
    "redis": "^3.0.2",
    "socket.io": "^2.3.0",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/connect-redis": "0.0.13",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookies": "^0.7.4",
    "@types/cors": "^2.8.6",
    "@types/csurf": "^1.9.36",
    "@types/express": "^4.17.2",
    "@types/express-rate-limit": "^5.0.0",
    "@types/express-session": "^1.17.0",
    "@types/formidable": "^1.0.31",
    "@types/helmet": "0.0.45",
    "@types/mongoose": "^5.7.10",
    "@types/morgan": "^1.9.0",
    "@types/node": "^10.17.24",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.18",
    "@types/socket.io": "^2.1.8",
    "@types/uuid": "^7.0.0",
    "copyfiles": "^2.3.0",
    "fork-ts-checker-webpack-plugin": "^5.0.6",
    "nodemon": "^1.19.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
}
